#
# Makefile for Ens_Reweight_wExpdatda program
#

prefix = @prefix@
HOME = @prefix@
SRC=@srcdir@
BIN=@bindir@
LIBDIR=@LDFLAGS@
INCDIR=@CPPFLAGS@

# definitions
CC    = @CC@

CFLAG = @CFLAGS@ 

OBJS =  ensRefEx.o error.o

LIBS =  error.h

EXLIBS = -llbfgs @LIBS@

TARGET = ensRefEx

.c.o:
	$(CC) $(INCDIR) -c $(CFLAG) $<;

all: ensRefEx

# rules of generations
ensRefEx:  $(OBJS) $(LIBS)
	$(CC) $(CFLAG) -o $@ $(OBJS) $(LIBDIR) $(INCDIR) $(EXLIBS) ;

install: 
	mkdir -p $(BIN); \
	cp $(TARGET) $(BIN) ; 

ensRefEx.o: error.h
error.o: error.h 

clean: 
	rm $(OBJS); \
	rm $(TARGET);
distclean:
	rm Makefile config.status config.cache config.log
